{
  "name": "sistema-avaliacao-psicologica",
  "version": "1.0.0",
  "description": "Sistema moderno de avaliação psicológica com base de dados",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:full": "node start-dev.js",
    "db:create": "node scripts/setup-database.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "setup": "npm run db:create && npm run migrate && npm run seed",
    "build": "cd frontend && npm run build",
    "build:copy": "npm run build && cp -r frontend/build ."
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "psicologia",
    "avaliacao",
    "testes",
    "database"
  ],
  "author": "Sistema de Avaliação Psicológica",
  "license": "MIT"
}
